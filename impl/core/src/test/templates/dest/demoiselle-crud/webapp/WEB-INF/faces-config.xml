<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <component>
        <component-type>viewSource</component-type>
        <component-class>teste.simplify.view.tag.ViewSource</component-class>
        <property>
            <property-name>sourceFile</property-name>
            <property-class>java.lang.String</property-class>
        </property>
        <property>
            <property-name>encoding</property-name>
            <property-class>java.lang.String</property-class>
        </property>
    </component>
    <managed-bean>
        <managed-bean-name>skinMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.SkinMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>skin</property-name>
            <value>blueSky</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>localeMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.LocaleMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pessoaMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.PessoaMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>paisMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.PaisMB</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>municipioMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.MunicipioMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ufMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.UFMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>planetaMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.PlanetaMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>continenteMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.ContinenteMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bairroMB</managed-bean-name>
        <managed-bean-class>teste.simplify.view.managedbean.BairroMB</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MyBeanMB</managed-bean-name>
        <managed-bean-class>com.mycompany.mypackage.view.managedbean.MyBeanMB</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <navigation-case>
            <from-outcome>pessoa_edit</from-outcome>
            <to-view-id>/private/pages/pessoa_edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pessoa_list</from-outcome>
            <to-view-id>/private/pages/pessoa_list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pais_crud</from-outcome>
            <to-view-id>/private/pages/pais_crud.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>municipio_edit</from-outcome>
            <to-view-id>/private/pages/municipio_edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>municipio_list</from-outcome>
            <to-view-id>/private/pages/municipio_list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>uf_edit</from-outcome>
            <to-view-id>/private/pages/uf_edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>uf_list</from-outcome>
            <to-view-id>/private/pages/uf_list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>planeta_list</from-outcome>
            <to-view-id>/private/pages/planeta_list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>continente_list</from-outcome>
            <to-view-id>/private/pages/continente_list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bairro_crud</from-outcome>
            <to-view-id>/private/pages/bairro_crud.xhtml</to-view-id>
        </navigation-case>
   		<navigation-case>
	        <from-outcome>mybean_crud</from-outcome>
	        <to-view-id>/private/pages/mybean_crud.xhtml</to-view-id>
	    </navigation-case></navigation-rule>
    <render-kit>
        <renderer>
            <component-family>demoiselle</component-family>
            <renderer-type>viewSourceRenderer</renderer-type>
            <renderer-class>teste.simplify.view.tag.ViewSourceRenderer</renderer-class>
            <attribute>
                <attribute-name>sourceFile</attribute-name>
                <attribute-class>java.lang.String</attribute-class>
            </attribute>
        </renderer>
    </render-kit>
    <lifecycle>
        <phase-listener>teste.simplify.view.listener.MessagePhaseListener</phase-listener>
    </lifecycle>
</faces-config>
